{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "my faceook React.js boilerplate",
  "main": "index.html",
  "scripts": {
    "dev": "watchify src/index.jsx -o bundle.js -v",
    "build": "browserify src/index.jsx > bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koba04/react-boilerplate"
  },
  "keywords": [
    "react"
  ],
  "author": "koba04",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koba04/react-boilerplate/issues"
  },
  "homepage": "https://github.com/koba04/react-boilerplate",
  "devDependencies": {
    "beefy": "^2.1.1",
    "browserify": "^5.11.0",
    "reactify": "^0.14.0",
    "watchify": "~1.0.2"
  },
  "dependencies": {
    "insert-css": "^0.2.0",
    "react": "^0.11.1",
    "stylify": "^0.1.6",
    "superagent": "^0.18.2",
    "backbone": "~1.1.2",
    "react-async": "~1.0.2",
    "react-router-component": "~0.20.1",
    "express": "~4.9.2",
    "express-react-views": "~0.5.0"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "harmony": true
        },
        {
          "everything": true
        }
      ],
      [
        "stylify"
      ]
    ]
  }
}
