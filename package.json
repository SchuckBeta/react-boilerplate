{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "my faceook React.js boilerplate",
  "main": "index.html",
  "scripts": {
    "dev": "beefy src/index.jsx:bundle.js --live",
    "build": "browserify src/index.jsx > bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koba04/react-boilerplate"
  },
  "keywords": [
    "react"
  ],
  "author": "koba04",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koba04/react-boilerplate/issues"
  },
  "homepage": "https://github.com/koba04/react-boilerplate",
  "devDependencies": {
    "beefy": "^2.1.1",
    "browserify": "^5.11.0",
    "reactify": "^0.14.0"
  },
  "dependencies": {
    "insert-css": "^0.2.0",
    "react": "^0.11.1",
    "react-router": "~0.7.0",
    "stylify": "^0.1.6",
    "superagent": "^0.18.2",
    "backbone": "~1.1.2"
  },
  "browserify": {
    "transform": [
      [ "reactify", { "es6": true }, { "everything": true } ],
      [ "stylify" ]
    ]
  }
}
